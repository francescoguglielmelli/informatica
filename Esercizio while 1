<button id="Bottone">Clicca</button>
<p id="output"></p>

<script>
  document.getElementById('Bottone').addEventListener('click', function() {
    var output = document.getElementById('output');
    var i = 1;
    while (i <= 100) {
      output.innerHTML += 'juve merda ' + i + '<br>';
      i++;
    }
  });
</script>

